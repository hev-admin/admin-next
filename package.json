{
  "name": "hev-admin",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1",
  "description": "",
  "author": "",
  "license": "MIT",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "postinstall": "npx simple-git-hooks",
    "start": "concurrently -n Server,Client -c bgBlue,bgCyan \"pnpm server:dev\" \"pnpm client:dev\"",
    "client:dev": "pnpm -F @hev-admin/client dev",
    "client:build": "pnpm -F @hev-admin/client build",
    "client:preview": "pnpm -F @hev-admin/client preview",
    "server:dev": "pnpm -F @hev-admin/server dev",
    "server:build": "pnpm -F @hev-admin/server build",
    "server:preview": "pnpm -F @hev-admin/server preview",
    "orm:migrate": "pnpm -F @hev-admin/server migrate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "pnpm -F @hev-admin/docs dev",
    "docs:build": "pnpm -F @hev-admin/docs build",
    "docs:preview": "pnpm -F @hev-admin/docs preview"
  },
  "dependencies": {
    "@unocss/reset": "^0.61.9",
    "axios": "^1.7.3",
    "dayjs": "^1.11.12",
    "vue": "^3.4.35",
    "vue-i18n": "^9.13.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@unocss/eslint-plugin": "^0.61.9",
    "concurrently": "^8.2.2",
    "eslint": "^9.8.0",
    "lint-staged": "^15.2.8",
    "sass": "^1.77.8",
    "simple-git-hooks": "^2.11.1",
    "vite": "^5.3.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
